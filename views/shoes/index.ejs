<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cop or Drop - Shoes</title>
    <link rel="stylesheet" href="/stylesheets/styles.css" />
  </head>
  <body>
    <%- include("../partials/_navbar.ejs") %>

    <h1>Cop or Drop page</h1>

    <main>
      <% shoes.forEach((shoe) => { %>
      <div>
        <a href="/shoes/<%= shoe._id %>">
          <img src="<%= shoe.image_url %>" />
          <p><%= shoe.model %></p>
        </a>
      </div>
      <% }) %>
    </main>
  </body>
</html>
